{
  "compilerOptions": {
    "target": "es6",   
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,    
    "esModuleInterop": true,    
    "moduleResolution": "node",    
    "module": "CommonJS"
  },
  "include": [    
    "lambdas/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ], 
  "types": [
    "lambda-types"
  ]
}
